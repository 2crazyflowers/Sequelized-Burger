{{#each burgers}}
	{{#unless this.devoured}}
		<div class="burger-div">
			<div class="row">
				<div class="col-md-7 col-7">
					{{this.burger_name}}
				</div>
				<div class="col-md-3 col-3">
					<button class="change-devour btn btn-outline-secondary rounded-0" data-id="{{this.id}}" data-newdevour="{{this.devoured}}">
						Devour It!
					</button>
				</div>
			</div>
		</div>
	{{/unless}}
{{/each}}